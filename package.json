{
  "name": "fsd-test-2",
  "version": "1.0.0",
  "scripts": {
    "clear": "rimraf ./build",
    "dev": "webpack-dev-server --mode-development --env development --open",
    "prod": "npm run clear && cross-env NODE_ENV=production webpack --mode-production --env production",
    "lint": "eslint \"./**/*.js\"",
    "deploy": "gh-pages -d build"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": {
    "production": [
      "defaults",
      "not IE 11",
      "not IE_Mob 11",
      "maintained node versions"
    ],
    "development": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 1 Safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alyonafomenkova/fsd-test-2.git"
  },
  "author": "Alena Fomenkova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alyonafomenkova/fsd-test-2/issues"
  },
  "homepage": "https://github.com/alyonafomenkova/fsd-test-2#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "gh-pages": "^3.1.0",
    "style-loader": "^1.1.4",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "air-datepicker": "^2.2.3",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "inputmask": "^5.0.5",
    "ion-rangeslider": "^2.3.1",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "normalize.scss": "^0.1.0",
    "postcss-loader": "^4.0.4",
    "pug": "^3.0.1",
    "pug-loader": "^2.4.0",
    "sass-loader": "^10.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.4.0"
  }
